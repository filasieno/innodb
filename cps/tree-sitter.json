{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "cps",
      "camelcase": "CPS",
      "title": "cps C",
      "scope": "source.cps",
      "file-types": [
        "cps"
      ],
      "injection-regex": "^cps$",
      "class-name": "TreeSitterCps"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Cps grammar for tree-sitter",
    "authors": [
      {
        "name": "Fabio N. Filasieno"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-cps"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}