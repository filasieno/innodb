{
  "name": "xinnodb-vscode-ext",
  "displayName": "XInnoDB VSCode Extension",
  "description": "Hello World starter command for the XInnoDB project.",
  "version": "0.0.1",
  "publisher": "your-publisher-id",
  "license": "MIT",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "xinnodb.helloWorld",
        "title": "XInnoDB: Hello World"
      }
    ]
  },
  "files": [
    "dist/**/*",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "watch": "tsc -w -p .",
    "lint": "echo \"No linter configured\"",
    "package": "vsce package",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.30",
    "@types/vscode": "^1.84.0",
    "@vscode/test-electron": "^2.3.9",
    "glob": "^10.3.10",
    "mocha": "^10.3.0",
    "typescript": "^5.4.0",
    "vsce": "^2.15.0"
  }
}


